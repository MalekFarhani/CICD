pipeline{
    agent any
stages{
stage('pulling the code'){
    steps{
        git branch: 'main', 
        url: 'https://github.com/MalekFarhani/TestingJavaApp'
}}
stage('build'){
    steps{ 
        sh 'mvn clean package'
}}
stage('docker'){
    steps{ 
        script{
        sh 'docker build . -t malekkkk/testing:0.0.1'
}}}


}}
